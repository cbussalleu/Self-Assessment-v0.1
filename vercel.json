{
  "rewrites": [
    {
      "source": "/api/typeform-webhook",
      "destination": "/api/typeform-webhook"
    }
  ],
  "headers": [
    {
      "source": "/api/typeform-webhook",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,OPTIONS"
        }
      ]
    }
  ]
}
